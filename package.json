  "repository": {
    "type": "git",
    "url": "git+https://github.com/formilyjs/@arco/web-vue.git"
  },
  "config": {
    "ghooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --ext .ts,.tsx,.js",
      "pretty-quick --staged",
      "git add"
    ],
    "*.md": [
      "pretty-quick --staged",
      "git add"
    ]
  },
